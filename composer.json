{
    "name": "chapa/chapa-laravel",
    "description": "A Laravel Package to Accept Payment through Chapa",
    "keywords": [
        "chapa",
        "chapa-laravel"
    ],
    "homepage": "https://github.com/chapa/chapa-laravel",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Kidus Yared",
            "email": "kidusy@chapa.co",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "classmap": [
            "src/Chapa.php"
        ],
        "psr-4": {
            "Chapa\\Chapa\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chapa\\Chapa\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chapa\\Chapa\\ChapaServiceProvider"
            ],
            "aliases": {
                "Chapa": "Chapa\\Chapa\\Facades\\Chapa"
            }
        }
    }
}
